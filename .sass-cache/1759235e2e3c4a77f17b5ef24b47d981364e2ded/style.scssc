/*This file is compiled in sass to the file /static/css/compiled/style.css*/

// Reset CSS for cross-browser consistency
@import "reset";

// image directory
$img_dir: "/static/img/";

html {
    background: url($img_dir + "experience_beyond_limit.jpg") no-repeat center center fixed; 
    -webkit-background-size: cover;
    -moz-background-size: cover;
    -o-background-size: cover;
    background-size: cover;
    width: 100%;
}

body {
    /* 'Merriweather Sans' is currently my favorite, but working in so many environments without internet connection is inspiring my to return to Ubuntu momentarily...plus, it's so beautiful... */
    font-family: Ubuntu, Helvetica, serif;
    color: #222;
    height: 100%;
    font-size: 115%;
}

p {
    //line-height: 125%;
    margin: 15px 5px;
}

$link_color: #1A4B6E;

$main_color: #F68026; //#FF7900;

a {
    color: $link_color;
    text-decoration: none;
    outline: 0;
    &:hover {
        color: #595;
    }
}

hr {
    //border: 2px solid;
}

ul { 
    list-style: none;
}

div {
    border-radius: 5px;
    -moz-border-radius: 5px;
    -webkit-border-radius: 5px;
}

textarea {
    border-radius: 5px;
}

button::-moz-focus-inner,
input[type="reset"]::-moz-focus-inner,
input[type="button"]::-moz-focus-inner,
input[type="submit"]::-moz-focus-inner,
input[type="file"] > input[type="button"]::-moz-focus-inner {
    border: none;
}

input[type="button"], input[type="submit"] {
    -moz-box-shadow: inset 0px 1px 0px 0px #ffffff;
    -webkit-box-shadow: inset 0px 1px 0px 0px #ffffff;
    box-shadow: inset 0px 1px 0px 0px #ffffff;
    background: -webkit-gradient( linear, left top, left bottom, color-stop(0.05, #ededed), color-stop(1, #dfdfdf) );
    background: -moz-linear-gradient( center top, #ededed 5%, #dfdfdf 100% );
    background-color: #ededed;
    -moz-border-radius: 6px;
    -webkit-border-radius: 6px;
    border-radius: 6px;
    border: 1px solid #dcdcdc;
    display: inline-block;
    color: #1c2b68;
    padding: 2px 24px;
    text-decoration: none;
    text-shadow: 1px 1px 0px #ffffff;
    font-weight: bold;
    cursor: pointer;
}

form.larger {
    font-size: larger;
}

.icon_button {
    background-size: 20px; 
    padding-left: 24px;
    height: 24px;
}

input.expand-button {
    @extend .icon_button;
    background: #ededed url($img_dir + "favicon.ico") no-repeat left;
}

input.edit-button {
    @extend .icon_button;
    background: #ededed url($img_dir + "icons/edit-icon.png") no-repeat left;
}

input.delete-button {
    @extend .icon_button;
    background: #ededed url($img_dir + "icons/delete-icon.png") no-repeat left;
    background-size: 20px; /* For some reason this isn't being inherited from parent class... */
}

input.invite-button {
    @extend .icon_button;
    background: #ededed url($img_dir + "icons/invite-icon.png") no-repeat left;
}

form {
    border-radius: 10px;
    border: 2px rgba(100, 100, 100, 0.1) solid;
    background-color: rgba(230, 230, 255, 0.5);
    padding: 3px;
    &.no_styling {
        border-radius: 0;
        border: none;
        background-color: transparent;
        padding: 0;
    }
}

textarea {
    background: url($img_dir + "/notepad_row.png") repeat;
    line-height: 25px;
    padding: 2px 10px;
    font-family: Ubuntu, Tahoma, sans-serif;
    font-size: 14px;
    min-height: 2em;
    border: hidden;
}

blockquote:after {
    content: " ";
    display: block;
    height: 0;
    clear: both;
}

blockquote .author {
    float: right;
    font-style: italic;
}

form.full_width input[type="text"] input[type="password"], textarea  {
    width: 100%;
    box-sizing: border-box;
    -moz-box-sizing: border-box;
    -webkit-box-sizing: border-box;
}

img.full_width {
    width: 100%;
}

#main {
    margin-top: 40px;
    clear: both;
}

.background_color {
    background-color: rgba(230, 230, 255, 0.92);
}

.shadow_border {
    -moz-box-shadow: 0 0 2px 1px #888;
    -webkit-box-shadow: 0 0 2px 1px #888;
    box-shadow: 0 0 2px 1px #888; 
}

#content {
    @extend .shadow_border;
    @extend .background_color;
    position: relative;
    float: left;
    width: 70%;
    padding: 5px;
    // & > div {
    //     padding: 3px;
    // }
}

#right_content {
    @extend .shadow_border;
    @extend .background_color;
    padding: 5px;
    position: fixed;
    margin-left: 65%;
    width: 25%;
    overflow-y: auto;
    max-height: 90%;
    div {
        padding: 5px;
    }
}

.object_item_list {
    position: relative;
}

.object_item {
    position: relative;
    padding: 5px;
    border-right: 2px solid rgba(74, 123, 158, 0.2);
    border-left: 2px solid rgba(74, 123, 158, 0.2);
    border-radius: 0.3em;
    margin: 2px;
    /*&:hover {
        border-right: 2px solid rgba(74, 123, 158, 0.6);
        border-left: 2px solid rgba(74, 123, 158, 0.6);
    }*/
}

.thirds_table {
    width: 100%;
}

.thirds_table td {
    vertical-align: top;
    width: 33%;
}

.thirds_table td > div {
    min-height: 300px;
}

$item_opacity: 0.3;

.explorer_item {
    @extend .object_item;
    background-color: rgba(74, 123, 138, $item_opacity);//rgba(131, 135, 141, $item_opacity);//
}

.experience_item {
    @extend .object_item;
    background-color: rgba(127, 95, 142, $item_opacity);//rgba(72, 61, 65, $item_opacity);//
    // min-height: 100px;
}

.narrative_item {
    @extend .object_item;
    background-color: rgba(54, 112, 60, $item_opacity); //rgba(88, 98, 97, $item_opacity);
    min-height: 100px;
    word-wrap: break-word;
}

.post_it {
    background: url($img_dir + "icons/tack-right-icon.png") no-repeat top right, url($img_dir + "icons/tack-left-icon.png") no-repeat top left,  url($img_dir + "/notepad_row.png");
}

.note {
    @extend .post_it;
    padding: 30px 10px 10px;
    min-height: 100px;
    margin-bottom: 10px;
}

.explorer_note {
    @extend .explorer_item;
    @extend .note;
    // border: 2px solid rgba(74, 123, 158, 1);
}

.experience_note {
    @extend .experience_item;
    @extend .note;
    // border: 2px solid rgba(167, 95, 142, 1);
}

.narrative_note {
    @extend .narrative_item;
    @extend .note;
    // border: 2px solid rgba(54, 152, 60, 1);
}

.experience_dash {
    @extend .experience_item;
    position: relative;
}

.request_dash {
    @extend .object_item;
    background-color: rgba(50, 80, 150, 0.5);
}

.left {
    float: left;
    width: 47%;
    margin: 5px;
    padding: 5px;
    background-color: rgba(10, 10, 10, 0.1);
}

.right {
    width: 47%;
    float: right;
    padding: 5px;
    margin: 5px;
    // background-color: rgba(10, 10, 10, 0.1);
}

.align_center {
    text-align: center;
}

.smaller_font {
    font-size: smaller;
}

.x-small_font {
    font-size: x-small;
}

.narrative_dash {
    @extend .narrative_item;
    min-height: 100px;
}

.narrative_taste {
    display: none;
}

.narrative_detail {
    @extend .narrative_item;
    min-height: 325px;
}

.narrative_detail .images_right {
    float: right;
    width: 30%;
    text-align: center;
    padding: 5px;
    img {
        display: block;
        margin-left: auto;
        margin-right: auto;
    }
}

.object_item_interaction {
    position: relative;
    bottom: 0px;
    left: 0px;
}

.comment_list {
    font-size: smaller;
    position: relative;
}

.comment textarea {
    
}

.youtube {
    padding: 5px;
}

#photo_form {
    width: 50%;
}

#photo_form_feedback {
    width: 40%;
    position: absolute;
    right: 0px;
    top: 0px;
}

.uploaded_photo {
    margin: 5px;
    float: left;
    width: 100px;
}

.gallery_photo_item {
    @extend .object_item;
    min-height: 290px;
    background-color: rgba(0, 0, 0, 0.1);
}

.featured_photo {
    text-align: center;
    img {
        max-width: 95%;
        display: inline-block;
        margin-left: auto;
        margin-right: auto;
        background-color: rgba(200, 200, 200, 0.3);
        border-radius: 0.55em;
    }
}

.gallery_photo_container {
    position: relative;
    min-height: 250px;
}

.previous_photo {
    position: absolute;
    left: 0;
    top: 0;
    width: 5%;
    background-color: rgba(100, 100, 100, 0.2);
    height: 90%;
    &:hover {
        background-color: rgba(100, 100, 100, 0.4);
    }
}

.previous_photo img {
    position: relative;
    top: 50%;
}

.gallery_photo {
    text-align: center;
    position: relative;
    height: 90%;
    width: 90%;
    left: 5%;
}

.gallery_photo img {
    position: relative;
    max-width: 85%;
}

.next_photo {
    position: absolute;
    right: 0;
    top: 0;
    width: 5%;
    height: 90%;
    background-color: rgba(100, 100, 100, 0.2);
    &:hover {
        background-color: rgba(100, 100, 100, 0.4);
    }
}

.next_photo img {
    position: relative;
    top: 50%;
}

.narrative_photo {
    margin-bottom: 25px;
}

.photo_caption {
    font-style: italic;
    text-align: center;
    font-size: smaller;
}

.caption {
    @extend .photo_caption;
}

.description {
    border-bottom: 1px dotted;
}

.icon_gallery_object {
    float: left;
}

.explorer_portrait {
    text-align: center;
}

.explorer_portrait img {
    width: 200px;
}

/* div for options near top of pages (e.g. experience: view photos, upload photo */
.option_list {
    @extend .object_item;
    overflow: hidden;
    background-color: rgba(150, 150, 150, 0.3);
    display: table;
    
}

.option_icon {
    padding-right: 7px;
    width: 26px;
    margin: 1px;
    &:hover {
        width: 28px;
        position:relative;
        margin: 0px; // hacky, but I kinda like it and it works wonderfully
    }
}

.toggle_div {
    // div initially hidden from view, expanded with jQuery in html template
    display: none;
}

/* div for the navigating of previous/next objects */
.next_previous {
    overflow: hidden;
}

.float_right {
    float: right;
}

.float_left {
    float: left;
}

.clear_both {
    clear: both;
}

.position_relative {
    position: relative;
}

.hidden {
    display: none;
}

.help_text {
    display: none;
    font-size: smaller;
    background-color: rgba(250, 250, 250, 0.15);
}

.pagination {
    margin-left: auto;
    margin-right: auto;
    text-align: center;
    width: 30%;
    background-color: rgba(74, 123, 138, $item_opacity);
    height: 50px;
    line-height: 50px;
    img {
        vertical-align: middle;
    }
}

.half {
    width: 50%;
}

.error {
    color: #B00;
    font-size: smaller;
}

.errorlist {
    @extend .error;
}

#wrapper {
    height: 100%;
    width: 90%;
    margin-right: auto;
    margin-left: auto;
}

#messages {
    @extend .shadow_border;
    font-size: smaller;
    position: fixed;
    width: 100%;
    text-align: center;
    background-color: rgba(200, 200, 200, 0.7);
    z-index: 3;
    top: 0px;
    left: 0px;
}

#messages_note {
    position: absolute;
    bottom: 0px;
    right: 20px;
}

#top_bar {
    @extend .shadow_border;
    position: fixed;
    top: 0px;
    left: 0px;
    width: 100%;
    background-color: $main_color; // previously: rgba(74, 123, 158, 0.9);
    z-index: 2;
    border-radius: 0em;
}

#top_bar_left {
    max-width: 75%;
    float: left;
}

#top_bar_right {
    max-width: 25%;
    position: absolute;
    right:150px;
}

#top_bar_signup {
    position: relative;
    top: 5px;
}

#logo {
    padding: 0px 30px;
    position: relative;
    float: left;
    color: #000;
    top: 8px;
}

#logo em {
    font-size: small;
    position: relative;
    bottom: 1px;
}

// #login_out {
//     position: relative;
//     right: 20px;
// }

.main_color {
    background-color: $main_color;
}

#id_comment {
    width: 100%;
    height: 2em;
    overflow: hidden;
}

#exp_comment_list {
    display: none;
}

#footer {
/*  margin-top: -50px;*/
    text-align: center;
    position: relative;
    clear: both;
    height: 50px;
    background-color: #AAA;
    opacity: 0.6;
}

#welcome_quote {
    padding: 10px;
    h2 {
        color: #5986A7;
    }
}

#featured_experience {
    // font-size: 18px;
    // background-color: rgba(167, 95, 142, 0.55);
    // padding: 3px;
}

.latest_narrative {
    @extend .narrative_item;
    max-height: 250px;
    overflow-y: auto;
    display: none;
}

#bulletin_board {
    position: relative;
    display: block;
    z-index: 1;
    padding: 5px;
}

#bulletin_board_background {
    content: "";
    background: url($img_dir + "bulletin_board_background.jpg");
    opacity: .5;
    top: 0;
    left: 0;
    bottom: 0;
    right: 0;
    position: absolute;
    z-index: -1;
}

.notifications {
    .notice {
        //@extend .object_item;
        @extend .note;
        //border: 2px solid $main_color;
    }
}

#new_narrative_form {
    display: none;
}

#id_caption {
    width: 350px;
    height: 100px;
}

#id_description {
    width: 350px;
    height: 100px;
}

.formatted_input {
    @extend .object_item;
    padding: 5px;
    margin: 10px;
    background-color: rgba(100, 100, 100, 0.2);
    input, select {
        min-height: 1em;
        font-size: 1em;
    }
    input, textarea {
        background-color: #F4DAFF;// #EEE;
        border-radius: 0.3em;
    }
}

.register_input {
    @extend .formatted_input;
    width: 350px;
    min-height: 70px;
}

#register_first_name {
    @extend .register_input;
    float: left;
}

#register_last_name {
    @extend .register_input;
    float: left;
}

#register_email {
    @extend .register_input;
    float: left;
}

#register_trailname {
    @extend .register_input;
    float: left;
}

#register_password1 {
    @extend .register_input;
    float: left;
}

#register_password2 {
    @extend .register_input;
    float: left;
}

#register_submit {
    clear: both;
}

#id_honeypot {
    display: none;
}

.my_nav ul {
    text-align: left;
    font-weight: bold;
    display: inline;
    margin-right: 15px;
    padding: 5px 9px 6px 0;
    list-style: none;
    -webkit-box-shadow: 0 0 5px rgba(0, 0, 0, 0.15);
    -moz-box-shadow: 0 0 5px rgba(0, 0, 0, 0.15);
    box-shadow: 0 0 5px rgba(0, 0, 0, 0.15);
    li {
        font: bold;// sans-serif;
        display: inline-block;
        margin-right: -12px;
        position: relative;
        padding: 5px;
        //background: #fff;
        cursor: pointer;
        -webkit-transition: all 0.2s;
        -moz-transition: all 0.2s;
        -ms-transition: all 0.2s;
        -o-transition: all 0.2s;
        transition: all 0.2s;
        a {
            color: #000;
        }
    }
}

.my_nav ul li:hover {
    background: rgb(74, 123, 138);
    color: #fff;

}

.my_nav ul li ul {
    padding: 0;
    position: absolute;
    top: 31px;
    left: 0;
    width: 150px;
    -webkit-box-shadow: none;
    -moz-box-shadow: none;
    box-shadow: none;
    display: none;
    opacity: 0;
    visibility: hidden;
    -webkit-transiton: opacity 0.2s;
    -moz-transition: opacity 0.2s;
    -ms-transition: opacity 0.2s;
    -o-transition: opacity 0.2s;
    -transition: opacity 0.2s;
    li { 
        background: rgb(74, 123, 138); 
        display: block; 
        color: #fff;
        padding: 10px;
        width: 11em;
        text-shadow: 0 -1px 0 #000;
        border-top: 1px #FFF solid; 
        a {
            color: #FFF;
        }
        &:hover {
            background: rgb(84, 133, 148);
        }
    }
}

.my_nav ul li:hover ul {
    display: block;
    opacity: 1;
    visibility: visible;
}
